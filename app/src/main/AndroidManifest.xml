<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cuisanzhang.mincreafting">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ActivityLoding"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityListViewShowBlocks"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityTip"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityListViewShowEnchants"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityWebViewFeedback"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivitySearch"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.cuisanzhang.mincreafting.GlideConfiguration"
            android:value="GlideModule" />

        <activity
            android:name=".FragmentMainActivity"
            android:screenOrientation="portrait">

        </activity>
    </application>

</manifest>